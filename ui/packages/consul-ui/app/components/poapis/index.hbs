                <span {{tooltip item.Service}}>
                  {{item.Service}}
                </span>
              </div>
            </B.Td>
            <B.Td class='po-table-cell'>
              <div class='po-table-cell-child-Host'>
                <span {{tooltip item.Host}}>
                  {{item.Host}}
                </span>
              </div>
            </B.Td>
            <B.Td class='po-table-cell'>
