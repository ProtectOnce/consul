<div {{did-insert this.handleWillRender}} class='po-container'>
  {{#if isLoaded}}
    <span class='display-flex-container'>
      <h1 class='api-endpoints-title'>API Endpoints</h1>
      <h5 class='powered-by'>
        Powered by
        <a class='powered-by-link' href='https://protectonce.com/' target='_blank'>
          {{! <img
        src='data:image/svg+xml;base64,'
        class='protectonce-logo'
      /> }}

          <img src='/ui/assets/images/poLogoFull.svg' class='protectonce-logo' />
        </a>
      </h5>
    </span>
    <Hds::Table>
      <:head as |H|>
        <H.Tr>
          <H.Th>Service</H.Th>
          <H.Th>Host</H.Th>
          <H.Th>Path</H.Th>
          <H.Th>Method</H.Th>
          <H.Th>Risk</H.Th>
          <H.Th>Issues</H.Th>
          <H.Th>PII</H.Th>
          <H.Th>Auth Type</H.Th>
          <H.Th>Internet Facing</H.Th>
          <H.Th>Traffic</H.Th>
          <H.Th>Drifted</H.Th>
        </H.Tr>
      </:head>
